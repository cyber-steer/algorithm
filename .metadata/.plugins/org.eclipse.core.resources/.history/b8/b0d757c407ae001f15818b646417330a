package time;

import java.util.Scanner;

public class Time4 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		long n = scanner.nextLong();
		int count = 0;
		for(int i=0; i<n-1; i++) {
			for(int j=i+1; j<n; j++) {
				System.out.println(i+" "+j);
			}
		}
		scanner.close();
	}
}
