package time;

import java.util.Scanner;

public class Time5 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		long n = scanner.nextLong();
		for(n=1; n<=10; n++) {
			long count = 0;
			for(int i=1; i<=n; i++) {
				for(int j=1; j<=n; j++) {
					for(int k=1; k<=n; k++) {
						count++;
					}
				}
			}
		}
		scanner.close();
	}
}
