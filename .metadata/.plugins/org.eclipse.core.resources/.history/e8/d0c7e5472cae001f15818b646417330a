package sort;

import java.util.Scanner;

public class Representative {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int sum = 0;
		int center = 0;
		for(int i=0; i<5; i++) {
			int n = scanner.nextInt();
			if(i==2) {
				center = n;
			}
			sum+=n;
		}
		System.out.println(sum/5);
		System.out.println(center);
	}
}
