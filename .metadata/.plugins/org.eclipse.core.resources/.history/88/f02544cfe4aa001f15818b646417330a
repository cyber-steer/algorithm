		String input = scanner.nextLine();
		int count = 0;
		for(int i=0; i<input.length(); i++) {
			char c = input.charAt(i);
			switch(c) {
				case 'c':{
					
				}
				case 'd':{
					
				}
				case 'l':{
					
				}
				case 'n':{
					
				}
				case 's':{
					
				}
				case 'z':{
					
				}
				default:
					count++;
			}
		}
		System.out.println(count);
	}
}
